<p id="notice"><%= notice %></p>

<h1>Listing Artists</h1>

    <% @artists.each do |artist| %>
        <%= artist.name %>
        <%= link_to 'Show', artist %>
        <%= link_to 'Edit', edit_artist_path(artist) %>
        <%= link_to 'Destroy', artist, method: :delete, data: { confirm: 'Are you sure?' } %>
    <% end %>

<br>

<%= link_to 'New Artist', new_artist_path %>
